import matplotlib.pyplot as plt
import numpy as np


#cosine = math.cos(x)
#sine = math.sin(x)

for n in range(1,5):
    x = np.linspace(-5,5,num=400)
    series = np.cos(2*np.pi/n*x)

plt.plot(series, 'r-', label= 'cos')

for n in range(1,5):
    y = np.linspace(-5,5,num=400)
    series1 = np.sin(2*np.pi/n*y)
plt.plot(series1, 'b--', label= 'sin')


plt.xlabel('Frequency')
plt.ylabel('Time')
plt.title('Sine and Cosine Wave')
plt.legend('CS')
plt.show()

